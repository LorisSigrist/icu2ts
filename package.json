{
    "name": "icu2ts",
    "version": "1.1.1",
    "description": "Generate TypeScript Types from ICU MessageFormat strings",
    "keywords": [
        "ICU",
        "Typesafe",
        "TypeScript",
        "MessageFormat",
        "i18n",
        "localization",
        "internationalization",
        "translation",
        "formatjs"
    ],
    "author": "Loris Sigrist",
    "type": "module",
    "types": "./types/index.d.ts",
    "main": "src/index.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/LorisSigrist/icu2ts.git"
    },
    "homepage": "https://github.com/LorisSigrist/icu2ts#readme",
    "license": "MIT",
    "scripts": {
        "test": "vitest --run --passWithNoTests",
        "format": "prettier . --write",
        "prepublishOnly": "node ./dts.js"
    },
    "dependencies": {
        "@formatjs/icu-messageformat-parser": "^2.6.2"
    },
    "devDependencies": {
        "@formatjs/intl": "^2.9.3",
        "dts-buddy": "^0.2.5",
        "prettier": "3.0.3",
        "vitest": "^0.34.5"
    },
    "files": [
        "src",
        "types"
    ],
    "exports": {
        ".": {
            "types": "./types/index.d.ts",
            "import": "./src/index.js"
        }
    }
}
